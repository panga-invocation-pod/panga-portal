%div.admin.index
  %h1 Users
  %div.group-actions
    = link_to "New User", new_admin_user_path, class: "btn btn-primary"
  %table.table
    %thead
      %tr
        %th{scope: "col"} Email
        %th{scope: "col"} Actions
    %tbody
      - @users.each do |user|
        %tr
          %td= user.email
          %td
            = link_to "edit", edit_admin_user_path(user)